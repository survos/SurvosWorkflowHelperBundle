{
    "name": "survos/workflow-extension-bundle",
    "description": "Add some tools for using the Symfony Workflow Component",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4",
        "symfony/workflow": ">= 5.3",
        "symfony/process": ">= 5.3",
        "symfony/console": ">= 5.3",
        "doctrine/orm": "^2.7|^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "survos/base-bundle": "dev-master",
        "symfony/config": ">= 5.3",
        "symfony/dependency-injection": "^6.0",
        "symfony/form": "^6.0",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": ">= 5.3",
        "symfony/twig-bundle": ">= 5.3"
    },

    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Survos\\WorkflowBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": {
        "survos_base_bundle": {
            "type": "vcs",
            "url": "git@github.com:survos/BaseBundle.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
