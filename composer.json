{
  "name": "survos/workflow-extension-bundle",
  "description": "Add some tools for using the Symfony Workflow Component",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.1",
    "doctrine/orm": "^2.7|^3.0",
    "symfony/console": "^6.1",
    "symfony/http-kernel": "^6.1",
    "symfony/messenger": "^6.1",
    "symfony/process": ">= 5.4",
    "symfony/workflow": ">= 5.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2",
    "survos/base-bundle": "dev-master",
    "symfony/config": "^6.1",
    "symfony/dependency-injection": "^6.1",
    "symfony/form": "^6.1",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": ">= 5.4",
    "symfony/twig-bundle": ">= 5.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tac Tacelosky",
      "email": "tacman@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Survos\\WorkflowBundle\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "require": "^6.1",
      "allow-contrib": "true"
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": {
    "survos_base_bundle": {
      "type": "vcs",
      "url": "git@github.com:survos/BaseBundle.git"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
