{
    "name": "survos/workflow-extension-bundle",
    "description": "Add some tools for using the Symfony Workflow Component",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1",
        "symfony/workflow": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0.5 || ^5.0"
    },

    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Survos\\WorkflowBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
