{
  "name": "survos/workflow-helper-bundle",
  "description": "Add some tools for using the Symfony Workflow Component",
  "type": "symfony-bundle",
  "keywords": ["symfony-ux","workflow"],
  "require": {
    "php": ">=8.1",
    "doctrine/orm": "^2.7|^3.0",
    "symfony/console": "^6.1",
    "symfony/http-kernel": "^6.1",
    "symfony/messenger": "^6.1",
    "symfony/process": ">= 6.0",
    "symfony/workflow": ">= 6.0",
    "symfony/yaml": "^6.1"
  },
  "require-dev": {
    "ekino/phpstan-banned-code": "^1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.7",
    "rector/rector": "^0.13.8",
    "symfony/browser-kit": "^6.1",
    "symfony/config": "^6.1",
    "symfony/dependency-injection": "^6.1",
    "symfony/form": "^6.1",
    "symfony/framework-bundle": "^6.1",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": "^6.1",
    "symfony/twig-bundle": "^6.1",
    "symfony/var-dumper": "^6.1",
    "symfony/webpack-encore-bundle": "^1.11"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tac Tacelosky",
      "email": "tacman@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Survos\\WorkflowBundle\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "require": "^6.1",
      "allow-contrib": "true"
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
